function divisible(n) {
    for (var i = 1; i <= 20; i++) {
        if (n % i != 0) {
            return false;
        }
    }

    return true;
}

function find() {
    var num = 2520;
    var flag = false;

    while (!flag) {
        num++;
        flag = divisible(num);
    }

    document.write(num);
}


find();
